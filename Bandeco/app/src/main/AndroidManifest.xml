<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sherolero.bandeco.sample"
    android:versionCode="1"
    android:versionName="1.1.1">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_bandeja"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >

        <activity
            android:name="com.sherolero.bandeco.sample.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:label="@string/shero" android:name="com.sherolero.bandeco.sample.SobreActivity"/>
        <activity android:label="@string/shero" android:name="com.sherolero.bandeco.sample.SaldoActivity"/>

    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
